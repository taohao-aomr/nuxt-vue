#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/nuxt@3.12.3_@opentelemetry+api@1.9.0_@types+node@20.14.9_eslint@9.6.0_sass@1.77.6_typescript@5.5.3_vite@5.3.3/node_modules/nuxt/bin/node_modules:/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/nuxt@3.12.3_@opentelemetry+api@1.9.0_@types+node@20.14.9_eslint@9.6.0_sass@1.77.6_typescript@5.5.3_vite@5.3.3/node_modules/nuxt/node_modules:/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/nuxt@3.12.3_@opentelemetry+api@1.9.0_@types+node@20.14.9_eslint@9.6.0_sass@1.77.6_typescript@5.5.3_vite@5.3.3/node_modules:/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/nuxt@3.12.3_@opentelemetry+api@1.9.0_@types+node@20.14.9_eslint@9.6.0_sass@1.77.6_typescript@5.5.3_vite@5.3.3/node_modules/nuxt/bin/node_modules:/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/nuxt@3.12.3_@opentelemetry+api@1.9.0_@types+node@20.14.9_eslint@9.6.0_sass@1.77.6_typescript@5.5.3_vite@5.3.3/node_modules/nuxt/node_modules:/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/nuxt@3.12.3_@opentelemetry+api@1.9.0_@types+node@20.14.9_eslint@9.6.0_sass@1.77.6_typescript@5.5.3_vite@5.3.3/node_modules:/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
