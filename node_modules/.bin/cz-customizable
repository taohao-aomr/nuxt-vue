#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/cz-customizable@7.0.0/node_modules/cz-customizable/node_modules:/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/cz-customizable@7.0.0/node_modules:/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/cz-customizable@7.0.0/node_modules/cz-customizable/node_modules:/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/cz-customizable@7.0.0/node_modules:/Users/taohao/Vite/nuxt-vue/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cz-customizable/standalone.js" "$@"
else
  exec node  "$basedir/../cz-customizable/standalone.js" "$@"
fi
